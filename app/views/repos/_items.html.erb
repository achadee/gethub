<table>
  <tr>
    <th>id</th>
    <th>Name</th>
    <th>Repo</th>
    <th>Username</th>
  </tr>
<% @repo_response[:repos].each do |repo| %>
  <tr>
    <td><%= repo.id %></td>
    <td><%= repo.name %></td>
    <td><a href="<%= repo.html_url %>"><%= repo.full_name %></a></td>
    <td><%= repo.user_name %></td>
  </tr>
<% end %>
</table>
